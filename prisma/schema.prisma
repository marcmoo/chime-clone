generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DB_HOST")
}

model User {
  id         String @id @map("_id")
  firstName  String
  lastName   String @map("secondName")
  email      String @unique
  password   String

  @@map(name: "users")
}
